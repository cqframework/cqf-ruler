{
	"java.configuration.updateBuildConfiguration": "automatic",
	"java.debug.settings.stepping.skipClasses": [
		"$JDK",
		"java.lang.ClassLoader",
		"org.springframework.aop.*"
	],
	"java.errors.incompleteClasspath.severity": "ignore",
	"java.test.config": [
		{
			"name": "test-config",
			"vmArgs": [
				"-XX:TieredStopAtLevel=1",
				"-Ddebug=true",
				"-Dloader.debug=true"
			]
		}
	],
	"java.test.defaultConfig": "test-config",
	"java.checkstyle.configuration": "${workspaceFolder}/config/checkstyle/checkstyle.xml",
	"java.checkstyle.version": "9.2.1",
	"cSpell.enabledLanguageIds": ["java", "json", "xml", "markdown"],
	"cSpell.ignorePaths": [
		".git/objects",
		".vscode",
		"**/pom.xml",
		"checkstyle.xml"
	],
	"cSpell.words": [
		"Alphora",
		"autoconfiguration",
		"autoconfigure",
		"Awaitility",
		"BCSEHEDISMY",
		"CAREGAP",
		"cdshooks",
		"Checkstyle",
		"classpath",
		"Codeable",
		"contentgroup",
		"DEQM",
		"drawio",
		"Dstu",
		"Eicrs",
		"FHIR",
		"Fulltext",
		"Gson",
		"h2",
		"Inclusivity",
		"Javadoc",
		"jdbc",
		"jpaserver",
		"loinc",
		"mgsc",
		"multiversion",
		"mvnw",
		"numer",
		"opencds",
		"pdmp",
		"Pointcut",
		"reindex",
		"SEARCHPARAMETER",
		"Servlet",
		"snomed",
		"springframework",
		"SUPPLEMENTALDATA",
		"Vals",
		"valueset",
		"valuesets",
		"vsecc",
		"Zulip"
	],
	"files.exclude": {
		"**/.classpath": true,
		"**/.project": true,
		"**/.settings": true,
		"**/.factorypath": true
	},
	"java.jdt.ls.vmargs": "-Xmx2G -XX:+UseG1GC -XX:+UseStringDeduplication",
	"java.compile.nullAnalysis.mode": "automatic"
}
