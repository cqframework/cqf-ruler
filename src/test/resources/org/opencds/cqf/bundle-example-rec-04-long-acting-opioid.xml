                  <valueString value="Today() - 4 months"/>
               </extension>
            </authoredOn>
            <dosageInstruction>
               <timing>
                  <repeat>
                     <frequency value="1"/>
                     <period value="3.0"/>
                     <periodUnit value="d"/>
                  </repeat>
               </timing>
               <asNeededBoolean value="false"/>
               <doseQuantity>
                  <value value="1.0"/>
                  <unit value="patch"/>
               </doseQuantity>
            </dosageInstruction>
            <dispenseRequest>
               <validityPeriod>
                  <extension url="http://hl7.org/fhir/StructureDefinition/cqif-cqlExpression">
                     <valueString value="FHIR.Period { start: FHIR.dateTime { value: Today() - 4 months }, end: FHIR.dateTime { value: Today() + 3 months } }"/>
                  </extension>
               </validityPeriod>
               <numberOfRepeatsAllowed value="3"/>
               <expectedSupplyDuration>
                  <value value="30.0"/>
                  <unit value="d"/>
               </expectedSupplyDuration>
            </dispenseRequest>
         </MedicationRequest>
      </resource>
      <request>
         <method value="PUT"/>
         <url value="http://measure.eval.kanvix.com/cqf-ruler/baseDstu3/MedicationRequest/example-rec-04-long-acting-opioid-prefetch"/>
